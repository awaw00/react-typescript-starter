{
  "name": "react-typescript-starter",
  "version": "0.0.1",
  "description": "Write your react application with typescript!",
  "main": "index.js",
  "scripts": {
    "start": "ts-node --project server server/index.ts",
    "clean": "rimraf dist",
    "build": "better-npm-run build",
    "build:client": "webpack",
    "build:server": "tsc --p server/tsconfig.json"
  },
  "betterScripts": {
    "build": {
      "command": "npm run build:client && npm run build:server",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:awaw00/react-typescript-starter.git"
  },
  "keywords": [
    "react",
    "typescript",
    "mobx",
    "mobx-react"
  ],
  "author": "awaw00 <awaw0618@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.0.0-alpha.7",
    "koa-connect-history-api-fallback": "^0.3.1",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.6.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "mobx": "^2.5.1",
    "mobx-react": "^3.5.5",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.32",
    "@types/koa-router": "^7.0.20",
    "@types/node": "^6.0.38",
    "@types/react": "^0.14.34",
    "@types/react-dom": "^0.14.16",
    "@types/react-router": "^2.0.33",
    "better-npm-run": "0.0.11",
    "config": "^1.21.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.13.0",
    "react-hot-loader": "^3.0.0-beta.3",
    "rimraf": "^2.5.4",
